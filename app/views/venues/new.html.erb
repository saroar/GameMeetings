<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Enter your Adress </h1>
  <%= simple_form_for @venue do |f| %>
    <%= f.input :address %><br />
    <%= f.input :latitude %><br />
    <%= f.input :longitude %><br />
    <br />
    <%= f.submit :CreateVenue, :class => 'btn btn-primary' %>
  <% end %>
    <br />
</div>
