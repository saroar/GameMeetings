<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Enter your Game Event details </h1>
  <%= simple_form_for @meeting do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :venue %>
    <%= f.input :game_date, :as => :date_picker %>
    <%= f.input :start_play_time, :as => :datetime_picker %>
    <%= f.input :end_play_time, :as => :datetime_picker %>
    <br />
    <%= f.submit :Create, :class => 'btn btn-primary' %>
  <% end %>
    <br />
</div>
