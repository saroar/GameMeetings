<h1 class="text-center">Welcome to GameEvents App :)</h1>
<br /><br />
<div class="text-center">
  <%= search_form_for @q do |f| %>
    <%= f.search_field :title_cont ,placeholder: 'Game Title' %>
    <%= f.search_field :description_cont ,placeholder: 'Game Description' %>
    <br /> <br/>
    <div class="actions">
      <%= f.submit "search" %>
    </div>
  <% end %>
</div>
<br /> <br />
<div class="row">
  <% @meetings.each do |meeting| %>
    <div class="col-md-4">
        <div class="booyah-box col-xs-10 col-xs-offset-1">
          <h1><%= meeting.title %></h1>
          <h2><%= meeting.description %></h2>
          
          <p><%= meeting.game_date %></p>
          <p><%= meeting.start_play_time %></p>
          <p><%= meeting.end_play_time %></p>
        </div>
      </div>
  <% end %>
</div>
